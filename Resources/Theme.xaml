<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    >
    <!-- SPECIAL COLOR -->
    <SolidColorBrush x:Key="RemoveBackground" Color="Transparent"/>
    <!-- COLOR -->
    <SolidColorBrush x:Key="MainText" Color="White"/>
    <SolidColorBrush x:Key="MainBackground" Color="#262B3C"/>
    <SolidColorBrush x:Key="GameWindow" Color="#26FFFFFF"/>
    <SolidColorBrush x:Key="InputLabel" Color="#CCFFFFFF"/>
    <SolidColorBrush x:Key="SummaryWindow" Color="#19D9D9D9"/>
    <SolidColorBrush x:Key="DefaultCellColor" Color="#CCFFFFFF"/>
    <SolidColorBrush x:Key="RightPosition" Color="#80FF72"/>
    <SolidColorBrush x:Key="WrongPosition" Color="#FFEE70"/>
    <SolidColorBrush x:Key="MissingPosition" Color="#E66760"/>
    

    <SolidColorBrush x:Key="Splitter" Color="White"/>

</ResourceDictionary>
